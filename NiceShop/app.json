{
  "expo": {
    "name": "NiceShop",
    "slug": "NiceShop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#48B600"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons.png",
        "backgroundColor": "#48B600"
      },
      "package": "com.evan_jake.NiceShop"
    },
    "web": {
      "favicon": "./assets/icons.png"
    },
    "extra": {
      "eas": {
        "projectId": "0963c5ac-6f9c-440c-82b7-44a3c0aec626"
      }
    }
  }
}
